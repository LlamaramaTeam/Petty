{
  "schemaVersion": 1,
  "id": "petty",
  "version": "${version}",
  "name": "Name Here",
  "description": "Mod Description",
  "authors": [
    "0xmycf",
    "KaloKYS",
    "Accieo"
  ],
  "contact": {
    "homepage": "https://github.com/LlamaramaTeam/petty",
    "issues": "https://github.com/LlamaramaTeam/petty/issues"
  },
  "license": "CC-BY-NC-SA 4.0",
  "icon": "assets/modid/icon.png",
  "environment": "*",
  "languageAdapters": {
    "custom": "org.llamarama.petty.language_adapter.CustomAdapter"
  },
  "entrypoints": {
    "main": [
      {
        "adapter": "custom",
        "value": "org.llamarama.petty.MainFile"
      }
    ],
    "client": [
      {
        "adapter": "custom",
        "value": "org.llamarama.petty.MainFileClient"
      }
    ]
  },
  "mixins": [
    "petty.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=16"
  }
}
